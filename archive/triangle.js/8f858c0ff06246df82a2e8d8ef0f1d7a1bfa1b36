/*8f858c0ff06246df82a2e8d8ef0f1d7a1bfa1b36*/function Triangle(e,d,f){this.a=e;this.b=d;this.c=f;this.slopeAB=Math.abs((e[1]-d[1])/(e[0]-d[0]));this.slopeAC=Math.abs((e[1]-f[1])/(e[0]-f[0]));this.slopeBC=Math.abs((d[1]-f[1])/(d[0]-f[0]))}Triangle.prototype.contains=function(d,l){var c=this.a[0],b=this.b[0],a=this.c[0],j=this.a[1],h=this.b[1],f=this.c[1],k=0,i=0,g=0,e=0;k=(b-c)*(f-j)-(a-c)*(h-j);i=((b-d)*(f-l)-(a-d)*(h-l))/k;g=((a-d)*(j-l)-(c-d)*(f-l))/k;e=((c-d)*(h-l)-(b-d)*(j-l))/k;return i>0&&g>0&&e>0};